{
    "name": "bordeux/geoname-bundle",
    "type": "symfony-bundle",
    "description": "Symfony GeoNameBundle",
    "keywords": ["geoname", "geonameid"],
    "homepage": "https://github.com/bordeux/geoname-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Krzysztof Bednarczyk",
            "email": "krzysztof@bednarczyk.me"
        }
    ],
    "require": {
        "php": ">=8.0",
        "symfony/framework-bundle": "~4.0|~5.0|~6.0|~6.1",
        "symfony/console": "~4.0|~5.0|~6.0",
        "symfony/dependency-injection": "~4.0|~5.0|~6.0|~6.1",
        "doctrine/doctrine-bundle": "^2.0",
        "guzzlehttp/guzzle": "^6.0|^7.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "php-coveralls/php-coveralls": "^2.5.2",
        "phpunit/phpunit": "^9.5.20",
        "doctrine/orm": "^2.5",
        "symfony/yaml": "~4.0|~5.0|~6.0|~6.1"
    },
    "autoload": {
        "psr-4": { "Bordeux\\Bundle\\GeoNameBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "Bordeux\\Bundle\\GeoNameBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
